/* pages/tarbar/home/index.wxss */
.nav-bar{
  position:fixed;
  top:0;
  width:100%;
  background-color: #C3D939;
  color: #fff;
  z-index: 22;
  font-size: 14px;
}
.nav-bar_title{
  position:absolute;
}

.location{
  color: #ffffff;
  font-size: 26rpx;
  align-items:center;
}
.location text{
  display: inline-block;
  max-width: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 30rpx;
}
.location-arrow{
  width: 0;
  height: 0;
  border-left: 9rpx solid transparent;
  border-right: 9rpx solid transparent;
  border-top: 10rpx solid #fff;
  margin:0 0 0 10rpx;
}
.location-icon{
  width: 21rpx;
  margin:0 10rpx 0 0;
}
.header-wrap{
  position: relative;
  transition: all 0.3s linear 0s;
  background:linear-gradient(0deg,rgba(255,255,255,1),rgba(195,217,57,1));
  padding:22rpx 20rpx 0;
}
.header{  
  background-color: #fff;
  border-radius: 20rpx 20rpx 0 0;
  padding:26rpx 20rpx;
}
.search{
  background-color: #F3F3F3;
  position: relative;
  font-size: 24rpx;
  border-radius: 32rpx;
  height: 64rpx;
  align-items: center;
  padding: 0 30rpx;
  color: #999;
}
.search image{
  width: 28rpx;
  margin:0 15rpx 0 0;
}

/* 通知 */
.notice{
  align-items:center;
  background-color:#fff;
  padding:0 10rpx;
}
.notice_img{
  font-size:0rpx;
  padding:0 17rpx 0 0;
}
.notice_img image{
  width: 24rpx;
}
.notice_box{
  flex: 1;
  font-size:24rpx;
  color:#666;
  height:72rpx;
  line-height:72rpx;
  margin: 0;
}
.notice_box swiper-item{
  text-overflow:ellipsis;
  white-space:nowrap;
}
.notice_con{
  white-space: nowrap;
}

.swiper-item{
  height: 100%;
  border-radius: 10rpx;
  overflow: hidden;
  align-items: center;
  justify-content: center;
}
.swiper-item image{
  width: 100%;
  height: 100%;
}
.main{
  position: relative;
  padding: 0 20rpx 20rpx;
}
.common{
  background-color: #fff;
  border-radius: 10rpx;
  margin:20rpx 0 0;
}
.common-header .weui-cell{
  padding:31rpx 30rpx 23rpx;
  align-items: center;
}
.common-header .weui-cell__bd{
  font-size: 30rpx;
  font-weight: 500;
  display: flex;
  align-items: center;
}
.common-header .weui-cell__bd::before{
  content: "";
  width:6rpx;
  height:30rpx;
  background-color: #C3D939;
  margin:0 16rpx 0 0;
}
.common-header .weui-cell__ft{
  color: #666;
  font-size:24rpx;
  padding:0 30rpx 0 0;
}

.main>image{
  width: 100%;
  height: 340rpx;
  position: absolute;
  top: 0;
}
.scroll{
  transition: all 0.3s linear 0s;
}
.scroll-top{
  width: 92.5%;
  position: fixed;
  top: 0;left: 0;right: 0;
  z-index: 9999;
  padding: 0 0 27rpx 0;
  margin: auto;
  background-color: #ffffff;
  transition: all 0.3s linear 0s;
}
.scrolls{
  width: 92.5%;
  position: fixed;
  top: 0;left: 0;right: 0;
  z-index: 999;
  padding: 0 0 20rpx 0;
  margin: auto;
  transition: all 0.3s linear 0s;
  background: #C3D939;
}



/* 菜单 */
.menu{
  padding: 28rpx;
  align-items: center;
  background-color: #ffffff;
  flex-wrap: wrap;
}
.menu_list{
  width: 20%;
  text-align: center;
  /* padding:0 20rpx; */
  font-size:21rpx;
  color:#666;
  box-sizing:border-box;
}
.menu_list image{
  width:64rpx;
  height:64rpx;
  border-radius: 50%;
}
.menu_list view{
  margin:16rpx 0 0;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}


/* 人气推荐 */
