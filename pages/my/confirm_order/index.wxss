page{
  background-color: #F3F3F3;
  color: #333333;
}
#confirm_order{
  padding-bottom: 150rpx;
}
#confirm_order>image{
  width: 100%;
  position: absolute;
  top: 0;
  z-index: -9
}
.confirm_order-item{
  padding: 50rpx 30rpx 0 30rpx;
}
/* 联系人信息 */
.contacts{
  padding: 0 20rpx;
  background-color: #ffffff;
  border-radius: 15rpx;
  margin-bottom: 20rpx;
}
.contacts_title{
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 2rpx solid #F3F3F3;
  font-weight: bold;
  font-size: 32rpx;
  position: relative;
  padding: 0 30rpx;
}
.contacts_title::after{
  content: "";
  position: absolute;
  left: 0;top: 30rpx;
  width: 8rpx;
  height: 30rpx;
  background-color: #C3D939;
}
.contacts_con{
  padding: 20rpx 0;
}
.contacts_con_view{
  font-size: 32rpx;
  color: #333333;
  font-weight: bold;
  margin-bottom: 10rpx;
  word-break:break-all;
}
.contacts_con_view input{
  display: inline-block;
}
.contacts_con_view input:nth-child(1){
  width: 125rpx;
  margin: 0 30rpx 0 0;
}
.contacts_con_view input:nth-child(2){
  width: 250rpx;
}
.contacts_con text{
  color: #666666;
  font-size: 22rpx;
  word-break:break-all;
}
.contacts_con_tiem{
  font-size: 26rpx;
  color: #666666;
}
.contacts_con_tiem text{
  color: #EF291A;
}
.contacts_con_tiem image{
  width: 13rpx;
  margin:0 0 5rpx 10rpx;
}
.contacts_con_address{
  color: #666666;
  font-size: 22rpx;
  line-height: 70rpx;
}
.contacts_con_address text:nth-child(2){
  margin-right: 10rpx;
}
.contacts_con_address text:nth-child(3){
  color: #EF291A;
  margin-left: 10rpx;
}
.contacts_con_address image{
  width: 17rpx;
  margin: 0 10rpx 0 0
}
.weui-flex{
  padding: 20rpx 0;
}
.contacts_tiem{
  line-height: 70rpx;
  font-size: 26rpx;
  color: #333333;
}
.contacts_tiem view{display: inline-block;width: 87%;}
.contacts_tiem image{
  width: 30rpx;
  margin: 0 15rpx 5rpx 0;
}
.goods_image{
  width: 110rpx;
  height: 110rpx;
}
.goods_image image{
  width: 100%;
  height: 100%;
}
.goods_details{
  padding-left: 20rpx;
  flex: 1;
}
.goods_title{
  font-size: 30rpx;
  line-height: 50rpx;
  max-width: 400rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  display: inline-block;
}
.goods_information{
  font-size: 26rpx;
  color: #999999;
}
.information_price{
  color: #EF291A;
  margin-right: 30rpx;
}
.prime_cost{
  text-decoration:line-through;
}
.information_num{
  float: right;
}
.goods_total{
  border-top: 2rpx solid #F3F3F3;
  padding: 20rpx 0;
  color: #333333;
  font-size: 26rpx;
  overflow: hidden;
}
.goods_total view{
  height: 60rpx;
  line-height: 60rpx;
}
.goods_coupon text{
  float: right;
  color: #666666;
}
.goods_coupon_selse{color: #EF291A !important;}
.goods_coupon image{
  width: 30rpx;
  float: right;
  margin: 16rpx 0 0 10rpx;
}
.goods_coupon input{
  width: 450rpx;
  float: right;
  text-align: right;
  margin-top: 10rpx;
}
.payment{
  text-align: right;
  margin-top: 20rpx;
  font-size: 26rpx;
  color: #666666;
}
.payment view{display: inline-block;}
.payment text{  
  color: #F03E33;
  font-size: 35rpx;
}
.order_details{
  color: #999999;
  font-size: 30rpx;
  padding: 20rpx 0;
}
.order_details view{
  height: 80rpx;
  line-height: 80rpx;
}
.order_details text{
  color: #333333;
  float: right;
}

/* 操作按钮 */
#footer{
  width: 93%;
  padding: 3.5%;
  background-color: #ffffff;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  z-index: 999;
}
#footer button{
  float: right;
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0;
  width: 200rpx;
  border-radius: 40rpx;
  margin-left: 20rpx;
}
.footer_phone{
  color: #ffffff;
  background-color: #EF291A;
}
#footer navigator{
  float: right;
}

/* 优惠券样式 弹窗*/
.screen_nav{
  width: 100%;
  display: inline-block;
  margin-top: 30rpx;
}
.screen_nav .li{
  width: 50%;
  float: left;
  text-align: center;
  font-size: 22rpx;
  position: relative;
  line-height: 60rpx;
}
.screen_nav .av_li::after{
  content: "";
  position: absolute;
  bottom: -10rpx;
  width: 60rpx;
  left: 40%;
  height: 6rpx;
  background-color: #FE0000;
}
.coupon{
  height:800rpx;
  overflow: scroll;
  background-color: #F6F7F9;
  padding-top: 20rpx;
}
.coupon_list{
  margin: 0 3.5% 30rpx 3.5%;
  border-radius: 15rpx;
  padding: 5rpx;
  position: relative;
  z-index: 9;
}
.coupon_list-background{
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.coupon_left{
  width: 30%;
  text-align: center;
  color: #FC4825;
  margin: 30rpx 0;
}
.coupon_price{
  color: #EF291A;
  font-size: 26rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.coupon_price text{
  font-size: 60rpx;
  font-weight: bold;
}
.coupon_condition{
  font-size: 26rpx;
  color: 26rpx;
}

.coupon_con{
  width: 55%;
  padding: 60rpx 0 60rpx 30rpx;
  flex-direction: column;
  text-align: left;
}
.coupon_con view{
  font-size: 32rpx;
  color: #333333;
}
.coupon_con text{
  font-size: 24rpx;
  color: #999999;
}
.coupon_right{
  width: 7%;
  padding: 60rpx 2.5% 60rpx 6.5%;
  font-size: 30rpx;
  color: #ffffff;
  text-align: center;
}
.coupon_right image{margin-top: 30rpx;}
.coupon_no{
  color: #707070;
  font-size: 22rpx;
  line-height: 60rpx;
  text-align: left;
  padding-left: 20rpx;
}
.screen_no{
  color: #707070;
  font-size: 26rpx;
  text-align: center;
  line-height: 100rpx;
}

/* 再次确认自提点 弹窗开始*/
.mask_cons{
  width: 80%;
  background-color: #ffffff;
  margin: 0 auto;
  margin-top: 40%;
  text-align: center;
  border-radius: 15rpx;
  overflow: hidden;
}
.mask_cons_title{
  font-size: 26rpx;
  line-height: 80rpx;
  position: relative;
  border-bottom: 2rpx solid #EEEEEE;
  font-weight: bold;
}
.mask_cons_title image{
  width: 40rpx;
  position: absolute;
  right: 25rpx;top: 20rpx;
}
.mask_cons_warp{
  padding: 20rpx 30rpx;
}
.cons_warp_viewTan{
  font-size: 30rpx;
  font-weight: bold;
  text-align: left;
  word-break:break-all;
}
.cons_warp_viewtan_text{
  color: #949494;
  font-size: 24rpx;
  text-align: left;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 4;
  -webkit-box-orient: vertical;
  word-break:break-all;
}
.cons_warp_addressTan{
  color: #C3D939;
  text-align: left;
  line-height: 70rpx;
  font-size: 26rpx;
}
.cons_warp_addressTan image{width: 25rpx;margin-right: 15rpx;}
.tan_people{
  font-size: 26rpx;
  line-height: 80rpx;
  border-top: 2rpx solid #EEEEEE;
  border-bottom: 2rpx solid #EEEEEE;
  text-align: left;
}
.tan_buttom{
  line-height: 80rpx;
  border-radius: 15rpx;
  color: #ffffff;
  background-color: #C3D939;
  font-size: 30rpx;
  margin-top: 50rpx;
}
/* 再次确认自提点 弹窗结束*/

.mask_tiem{
  background-color: #FFFFFF;
  height:90rpx;
  border-top: 2rpx solid #EEEDEF;
  text-align: center;
  line-height: 90rpx;
  font-size: 30rpx;
  padding: 0;
}
.mask_tiem view:nth-child(1){
  background-color: #C3D939;
  flex: 1;
  color: #FFFFFF;  
}
.mask_tiem view:nth-child(2){
  flex: 1;
  color: #666666;
}