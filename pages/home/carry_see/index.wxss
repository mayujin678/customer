/* pages/home/carry_see/index.wxss */
page,#carry_see{
  background-color: #ffffff;
  width: 100%;
  height: 100%;
}
.map{
  width: 100%;
  height: 100%;
}
.navigation{
  width: 93%;
  padding: 30rpx 3.5%;
  background-color: #ffffff;
  font-size: 30rpx;
  align-items: center;
  position: fixed;
  bottom: 0;
}
.navigation image{
  width: 50rpx;
}
.address{
  color: #666666;
  flex: 1;
  max-width: 500rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.navigation_buttom{
  width: 150rpx;
  line-height: 60rpx;
  color: #ffffff;
  background-color: #C3D939;
  text-align: center;
  border-radius: 15rpx;
}